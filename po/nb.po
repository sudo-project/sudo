#: src/parse_args.c:607
#, c-format
msgid ""
"%s - edit files as another user\n"
"\n"
msgstr ""
"%s - rediger filer som om du var en annen bruker\n"
"\n"

#: src/parse_args.c:609
#, c-format
msgid ""
"%s - execute a command as another user\n"
